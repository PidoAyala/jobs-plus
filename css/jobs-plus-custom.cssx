/**
* Custom CSS declarations
*
*/

.pro-profile-wrapper{
	position: relative;
	width: 100%;
	font-size: 14px;
	line-height: 24px;
	color: #030303;
}
.post-content .pro-profile-wrapper h3{
	color: #272727;
	font-size: 18px;
	font-weight: bold;
	border-bottom: 1px solid #c5c5c5;
	-moz-box-shadow: 0 1px 0 #fff;
	-webkit-box-shadow: 0 1px 0 #fff;
	box-shadow: 0 1px 0 #fff;
}
.pro-content-command {
	float: right;
	font-size: 16px;
}
.pro-content-command a {
	color: #38aad6;
	font-weight: bold;
	text-transform: uppercase;
}

.pro-profile-edit .hide-on-edit {
	display: none;
}
.show-on-edit {
	display: none;
}
.pro-profile-edit .show-on-edit {
	display: block;
}

.pro-left, .pro-content, .pro-right {
	padding: 48px 0 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

@media screen and (min-width: 800px) {
	#backdrop.full-width.job-board:after {
		content: " ";
		position: absolute;
		top: 0;
		right: 0;
		width: 50%;
		height: 100%;
		background: #eaeaea;
		z-index: -2;
	}
	#backdrop.full-width.job-board .post-content:after {
		content: " ";
		position: absolute;
		top: 0;
		left: 0;
		width: 74.609375%;
		height: 100%;
		background: #f4f4f4 url('../img/bg-noise.png') repeat;
		z-index: -1;
		-moz-box-shadow: 5px 0 5px rgba(0,0,0,.1);
		-webkit-box-shadow: 5px 0 5px rgba(0,0,0,.1);
		box-shadow: 5px 0 5px rgba(0,0,0,.1);
	}
	.pro-left{
		float: left;
		width: 20.703125%;
		margin-left: -74.609375%;
	}
	.pro-content{
		width: 53.90625%;
		margin-left: 20.703125%;
		float: left;
		padding-left: 40px;
		padding-right: 40px;
	}
	.pro-right{
		float: right;
		width: 25.390625%;
		vertical-align: top;
		padding-left: 30px;
	}
}

.pro-content-wrapper {
	margin: 0 0 36px;
}

.pro-right .pro-content-wrapper {
	margin-bottom: 48px;
}

.pro-name {
	font-size: 16px;
}


.pro-content label{
	margin: 0;
}

.pro-content label span{
	display: inline-block;
	vertical-align: top;
}
.pro-content label strong {
	display: inline-block;
	vertical-align: top;
	font-weight: bold;
}

.pro-content-editable {
	position: relative;
}
.pro-content-editable:before {
	content: " ";
	position: absolute;
	top: 0;
	left: -20px;
	width: 13px;
	height: 24px;
	background: url('../img/sprite-jobs.png') no-repeat 0 0;
	display: none;
}
.pro-profile-edit .pro-content-editable:before {
	display: block;
}
.pro-content-editable-open:before,
.pro-content-editable:hover:before {
	background-position: 0 -24px;
}

.pro-content-editable .editable,
.pro-content-editable .editable:hover,
.pro-content-editable .editable:focus {
	background: none;
	font-style: normal;
	font-weight: normal;
	color: #030303;
}
.pro-content-editable .editable-firstlast,
.pro-content-editable .editable-firstlast:hover,
.pro-content-editable .editable-firstlast:focus,
.pro-content-editable .editable-link,
.pro-content-editable .editable-link:hover,
.pro-content-editable .editable-link:focus {
	color: #217fb1;
}

.pro-biography .editable-container.editable-inline {
	width: 100%;
}

.pro-content .pro-social-media {
	font-size: 16px;
}
.pro-content .pro-social-media strong {
	width: 135px;
}
.pro-social-media .editable,
.pro-social-media .editable:hover,
.pro-social-media .editable:focus {
	font-weight: bold;
}
.pro-social-media .editable-container.editable-inline {
	display: block;
	width: auto;
	margin: -24px 0 0 135px;
}

.pro-editable-remove {
	display: block;
	width: 18px;
	height: 18px;
	background: url('../img/sprite-jobs.png') no-repeat 0 -96px;
	position: absolute;
	top: -9px;
	right: -8px;
}

.pro-button, .pro-button:hover,
.pro-contact, .pro-contact:hover,
.pro-points button, .pro-points button:hover {
	border: none;
	background-color: #37a9d5;
	-moz-box-shadow: 0 1px 0 rgba(4,38,64,.7), inset 1px 0 0 rgba(59,175,217,.75);
	-webkit-box-shadow: 0 1px 0 rgba(4,38,64,.7), inset 1px 0 0 rgba(59,175,217,.75);
	box-shadow: 0 1px 0 rgba(4,38,64,.7), inset 1px 0 0 rgba(59,175,217,.75);
	background-image: url(data:image/svg+xml;base64,);
	background-image: -moz-linear-gradient(bottom, #3db9e8 0%, #43ceff 100%);
	background-image: -o-linear-gradient(bottom, #3db9e8 0%, #43ceff 100%);
	background-image: -webkit-linear-gradient(bottom, #3db9e8 0%, #43ceff 100%);
	background-image: linear-gradient(bottom, #3db9e8 0%, #43ceff 100%);
	color: #fff;
	font-weight: bold;
	text-transform: uppercase;
	text-align: center;
	text-shadow: 0 -1px 0 #29aeee;
}
.pro-button-light, .image-label {
	border: 1px solid #c3c3c3;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	border-radius: 2px;
	background-color: #f3f3f3;
	background-image: url(data:image/svg+xml;base64,);
	background-image: -moz-linear-gradient(bottom, #e3e3e3 0%, #e3e3e3 0.76%, #fff 99.24%, #fff 100%);
	background-image: -o-linear-gradient(bottom, #e3e3e3 0%, #e3e3e3 0.76%, #fff 99.24%, #fff 100%);
	background-image: -webkit-linear-gradient(bottom, #e3e3e3 0%, #e3e3e3 0.76%, #fff 99.24%, #fff 100%);
	background-image: linear-gradient(bottom, #e3e3e3 0%, #e3e3e3 0.76%, #fff 99.24%, #fff 100%);
	color: #000;
	font-size: 14px;
	font-weight: bold;
	line-height: 20px;
	height: 30px;
	padding: 0 13px;
}

.pro-edit {
	float: right;
	margin-top: -30px;
	position: relative;
	z-index: 10;
}

.pro-go-public {
	text-align: right;
}

.pro-edit-button, .pro-go-public-button {
	width: 100px;
	height: 30px;
	line-height: 30px;
}

.pro-contact{
	max-width: 100%;
	width: 100%;
	height: 48px;
	line-height: 32px;
	font-size: 18px;
	float: none;
	margin: 0 0 48px;
}

.pros-feedback{
	background-color: #eee;
	padding: 10px 0 0 0px;
	padding-right: 130px;
	font-size: 10pt;
}

.fb-left{
	border: 1px solid #aaa;
	border-radius: 5px;
	background-color: #eee;
	height: 70px;
	width: 70px;
	float: left;
	padding: 5px;
	margin-top: 10px;
}
.fb-right{
	position:relative;
	background-color: #fff;
	border: 1px solid #aaa;
	border-radius: 5px;
	height: 60px;
	width: 100%;
	margin: 10px -130px 0 20px;
	display: inline-block;
	padding: 10px;
	box-shadow: 0 0 6px #B2B2B2;
	vertical-align: top;
}

.fb-right::after {
	border: 0px solid #aaa;
	background-color: #fff;
	content: "\00a0";
	display: block;
	height: 8px;
	position: absolute;
	top: 32px;
	left: -6px;
	transform:             rotate( 29deg ) skew( -35deg );
	-moz-transform:    rotate( 29deg ) skew( -35deg );
	-ms-transform:     rotate( 29deg ) skew( -35deg );
	-o-transform:      rotate( 29deg ) skew( -35deg );
	-webkit-transform: rotate( 29deg ) skew( -35deg );
	width:  10px;
	float: left;
	box-shadow: -2px 2px 2px 0 rgba( 178, 178, 178, .4 );

}


.fb-bottom{
	position: absolute;
	left: 10px;
	right: 10px;
	bottom: 0;
}
.fb-bottom span{
	float: right;
}
