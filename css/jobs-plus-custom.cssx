#backdrop.full-width.job-board {
	background: #f4f4f4 url('../img/bg-noise.png') repeat;
	position: relative;
}
#backdrop.full-width.job-board .tile {
	background: none;
}

.jobs-description{
  font-style: italic;	
}

.job-board .post-content {
	width: 100%;
	padding: 0;
	position: relative;
}

#pro-list,#project-list {
	font-size:75%;
}

.comments-link {
	font-weight:400;
}

.jobs_button {
	background-color: #000;   /*#69c2e4;*/
	border:0;
	border-left:1px solid #444;
	border-right:1px solid #888;
	color:#fff;
	font-size:.9em;
	letter-spacing:1px;
	line-height:1em;
	max-width:90%;
	width:20%;
	padding: 10px;
}

.jobs-nav .jobs_button{
	text-align:left;
	float: left;
	height:60px;
}

.jobs_button span {
	font-size: 1.1em;
}

.jobs_button.selected {
	background-color:#8ea3ac;
}

.jobs_button:hover {
	background:#043e64;
	color:#fff;
}

.jobs-tags{
	width: 96%;
}

.pro-detail {
	float:right;
	margin:0;
	padding:0;
	width:75%;
}

.pro-img {
	/* background-image:url(../img/default-pro-image.png);
	box-shadow:3px 3px 5px #CCC;
	margin:0; */
	float:left;
}

.pro-item {
	border-bottom:1px solid #DDD;
	float:left;
	font-size:13px;
	margin:0;
	padding:1em 0;
	width:100%;
}

.pro-item h2.ptitle {
	font-size:2em;
	line-height:1em;
	margin:0;
	padding:0;
}

.pro-title{
	clear: both;
	text-align: center;
	font-weight: bold;
	font: bold 22px "proxima-nova-condensed", sans-serif;
}

.detail-title {
	margin:1em;

}

.edit-link {
	background:url(../img/cil.png) no-repeat left center;
	float:right;
	padding-left:16px;
}

.editfield {
	margin-bottom:1em;
	padding:0;
}

.editfield input,.editfield select {
	font-size:1.2em;
	height:30px;
	margin:0;
	padding:2px;
	width:100%;
}

.editfield textarea {
	font-size:1.2em;
	margin:0;
	padding:0;
	width:100%;
}

.error {
	background-color:#FFEBE8;
	border:1px solid red!important;
	margin:.5em 0;
	padding:1em;
}


.job-budget,.job-date {
	float:left;
	margin:0;
	padding:0;
	width:25%;
}

.job-title {
	float:left;
	margin:0;
	padding:0;
	width:50%;
}


.jobs_button {
	min-width:6em;
}

.list-header {
	border-bottom:1px solid #DDD;
	color:#043e64;
	float:left;
	font:1.2em Amplitude,Arial,sans-serif;
	margin-top:1em;
	padding:.5em 2%;
	width:96%;
}

.pblock {
	border-bottom:1px solid #DDD;
	float:left;
	margin:0;
	padding:0;
	width:100%;
}

.pblock .meta-info {
	float:right;
	padding:1em;
	text-align:left;
	width:60%;
}

.pblock .meta-title {
	float:left;
	margin:0;
	padding:1em;
	width:20%;
}


.project-budget,.project-date {
	float:left;
	padding:0;
	width:24%;
}

.project-excerpt {
	clear:both;
	float:left;
	margin-top:1em;
	padding:0;
}

.project-item {
	border-bottom:1px solid #DDD;
	float:left;
	font-size:13px;
	margin:0;
	padding:1em 2%;
	width:96%;
}

.project-title {
	float:left;
	font-size:1.4em;
	margin:0;
	padding:0;
	width:50%;
}

.project-tos,.pro-tos {
	font-size:75%;
	height:10em;
	overflow:auto;
}

.tags {
	clear:both;
	font-size:small;
	left:0;
	margin:0;
}

.updated {
	background-color:#FFFFD5;
	border:1px solid #E6DB55!important;
	margin:.5em 0;
	padding:1em;
}

