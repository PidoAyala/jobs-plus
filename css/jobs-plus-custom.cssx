.gravatar{
	margin: 0;
	/*background: #c5bfb6 url("../img/default-pro-image.png") no-repeat scroll;*/
	border-radius: 7px;
	padding: 15px;
	position: relative;
}
.gravatar:before {
	content: " ";
	position: absolute;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	-moz-box-shadow: 0 1px 0 #fff, inset 0 1px 0 rgba(102,102,102,.2), inset 0 0 0 1px rgba(0,0,0,.03), inset 0 0 0 10px #ddd;
	-webkit-box-shadow: 0 1px 0 #fff, inset 0 1px 0 rgba(102,102,102,.2), inset 0 0 0 1px rgba(0,0,0,.03), inset 0 0 0 10px #ddd;
	box-shadow: 0 1px 0 #fff, inset 0 1px 0 rgba(102,102,102,.2), inset 0 0 0 1px rgba(0,0,0,.03), inset 0 0 0 10px #ddd;
	border-radius: 7px;
}
.gravatar-border {
	-moz-box-shadow: 0 0 0 12px #ddd;
	-webkit-box-shadow: 0 0 0 12px #ddd;
	box-shadow: 0 0 0 12px #ddd;
	border-radius: 9px;
}

.avatar img{
	width: 100%;
	height: 100%;
	margin: 0;
	-moz-box-shadow: inset 0 1px 0 rgba(102,102,102,.2), inset 0 0 0 1px rgba(0,0,0,.03);
	-webkit-box-shadow: inset 0 1px 0 rgba(102,102,102,.2), inset 0 0 0 1px rgba(0,0,0,.03);
	box-shadow: inset 0 1px 0 rgba(102,102,102,.2), inset 0 0 0 1px rgba(0,0,0,.03);
}

.gravatar img{
	width: 100%;
	height: 100%;
	border-radius: 9px;
