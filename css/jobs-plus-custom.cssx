.prj-square{ /* Use for square boxes */
	position: relative;
	width: 24%;	/* must be half the rectangle width*/
	display: inline-block;
	font-weight: bold;
	margin-right: .5%;
}

.prj-rectangle{ /* Use for rectangle boxes */
	position: relative;
	width: 49%;	/* Must be twice the square width*/
	display: inline-block;
	margin-right: .5%;
}

.prj-item-full{
	margin: 0 1em;
}

.prj-full{
	position: relative;
	width: 99%;
	float: left;
	overflow: hidden;
}

.prj-square > img.sizer{
	display: block;
	width: 100%;
	height: auto;
}

.prj-rectangle > img.sizer{
	display: block;
	width: 98%; /* fine adjustment of rectangle size */
	height: auto;
}

.prj-square > .prj-placeholder{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 170px;
	line-height: 100%;
	display: table;
	text-align: center;
}

.prj-placeholder span{
	display: table-cell;
	vertical-align: middle;
	line-height: normal;
	text-align: center;
}

.prj-footer{
	font-size: 75%;
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	width: auto;
	padding: 0 1em 0 1em;
