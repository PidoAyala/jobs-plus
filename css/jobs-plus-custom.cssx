/* Skills gauge */


.skill-bar{
	border: 1px solid #c5c5c5;
	background-color: #f3f3f3;
}

.editable-skill .ui-slider-range-min,
.skill-percent{
	height: 13px;
	background-color: #e23de9;
	-moz-box-shadow: inset 0 4px 4px rgba(0,0,0,.1), inset 0 0 4px rgba(0,0,0,.21);
	-webkit-box-shadow: inset 0 4px 4px rgba(0,0,0,.1), inset 0 0 4px rgba(0,0,0,.21);
	box-shadow: inset 0 4px 4px rgba(0,0,0,.1), inset 0 0 4px rgba(0,0,0,.21);
	background-image: url(data:image/svg+xml;base64,);
	background-image: -moz-linear-gradient(left, #e93d8d 0%, #e23de9 100%);
	background-image: -o-linear-gradient(left, #e93d8d 0%, #e23de9 100%);
